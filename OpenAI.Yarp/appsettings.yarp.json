{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "openai/{**catch-all}"
        },
        "Transforms": [
          {
            "PathRemovePrefix": "/openai"
          }
        ]
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "api.openai.com": {
            "Address": "https://api.openai.com/"
          }
        }
      }
    }
  }

}
